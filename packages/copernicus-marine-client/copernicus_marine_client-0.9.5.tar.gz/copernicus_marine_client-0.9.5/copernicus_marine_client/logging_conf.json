{
  "disable_existing_loggers": false,
  "formatters": {
    "blank": {
      "format": "%(message)s"
    },
    "simple": {
      "format": "%(levelname)-8s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "simple",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    },
    "console_blank": {
      "class": "logging.StreamHandler",
      "formatter": "blank",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "blank_logger": {
      "handlers": [
        "console_blank"
      ],
      "level": "INFO",
      "propagate": false
    }
  },
  "root": {
    "handlers": [
      "console"
    ],
    "level": "INFO"
  },
  "version": 1
}
